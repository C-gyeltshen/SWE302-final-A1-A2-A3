{
  "name": "react-redux-realworld-example-app",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@playwright/experimental-ct-react": "^1.56.1",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.5.0",
    "babel-plugin-istanbul": "^7.0.1",
    "cross-env": "^5.1.4",
    "dom-accessibility-api": "^0.5.16",
    "nyc": "^17.1.0",
    "react-scripts": "1.1.1",
    "redux-mock-store": "^1.5.4"
  },
  "dependencies": {
    "history": "^4.6.3",
    "marked": "^0.3.6",
    "prop-types": "^15.5.10",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.1",
    "superagent": "^3.8.2",
    "superagent-promise": "^1.1.0"
  },
  "scripts": {
    "start": "cross-env PORT=4100 react-scripts start",
    "build": "react-scripts build",
    "test": "cross-env PORT=4100 react-scripts test --env=jsdom",
    "test:playwright": "playwright test",
    "test:playwright:unit": "playwright test test/reducers",
    "test:playwright:e2e": "playwright test test/components",
    "test:playwright:ui": "playwright test --ui",
    "test:playwright:headed": "playwright test --headed",
    "test:playwright:debug": "playwright test --debug",
    "test:playwright:report": "playwright show-report",
    "test:coverage": "rm -rf .nyc_output coverage && nyc --reporter=html --reporter=text --reporter=lcov playwright test test/reducers",
    "test:coverage:all": "rm -rf .nyc_output coverage && BABEL_ENV=test nyc --reporter=html --reporter=text --reporter=lcov playwright test",
    "test:coverage:report": "nyc report --reporter=html --reporter=text",
    "coverage:view": "open coverage/index.html",
    "eject": "react-scripts eject"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
}
